for n in range(2, 100000):
    n_s = str(n)
    s1 = 0 
    s2 = 0 
    while n != 0:
        b = n % 10 
        if b % 2 != 0:
            s1 += b 
        n //= 10
    for i in range(len(n_s)):
        if (i + 1) % 2 == 0:
            s2 += int(n_s[i])
    if abs(s1 - s2) == 29:
        print(n)
        break
